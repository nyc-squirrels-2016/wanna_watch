<%= form_tag sessions_path do %>
	<button type="button" data-div-type="close_login_button" class="button" >X</button>
  <p><%=text_field_tag :email, nil, placeholder: "email"%></p>
  <p><%=password_field_tag :password, nil, placeholder: "password" %></p>
  <p><%=submit_tag "Let's Watch" %></p>
<%end%>
